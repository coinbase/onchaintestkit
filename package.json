{
  "name": "@coinbase/onchaintestkit",
  "version": "0.1.0",
  "description": "End-to-end testing toolkit for blockchain applications, powered by Playwright",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "prepare-metamask": "./src/cli/prepare-metamask.mjs"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "format": "biome format --write .",
    "format:check": "biome format --check .",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "test": "playwright test",
    "prepare-metamask": "node src/cli/prepare-metamask.mjs",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "blockchain",
    "testing",
    "e2e",
    "playwright",
    "web3",
    "wallet",
    "metamask",
    "coinbase-wallet"
  ],
  "author": "Coinbase",
  "license": "MIT",
  "peerDependencies": {
    "@playwright/test": "^1.34.0"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^0.36.4",
    "ethers": "^5.7.2",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.1.1",
    "node-fetch": "^3.3.2",
    "viem": "^1.10.9"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@playwright/test": "^1.34.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "workspaces": [
    "."
  ],
  "exports": {
    ".": "./src/index.ts",
    "./src/*": "./src/*"
  },
  "private": true
}
